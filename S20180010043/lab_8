import java.util.*;

class Address
{
int pincode;
String city;

Address(int p,String c)
{
pincode=p;
city=c;
}

// get and set pincode
public int getpin()
{
return pincode;
}
public void setpin(int newP)
{
this.pincode=newP;
}

//get and set city
public String getcity()
{
return city;
}
public void setcity(String c)
{
this.city=c;
}

}

class Person
{
String name;
Address addr;
int aadhar_no;

//constructor
Person(String n,Address address,int a)
{
name=n;
aadhar_no=a;
addr=address;
}

//get and set name
public String getname()
{
return name;
}
public void setname(String n)
{
this.name=n;
}

//get and set aadhar
public int getaadhar()
{
return aadhar_no;
}
public void setaadhar(int n)
{
this.aadhar_no=n;
}
}

class Software_Engg extends Person
{
int s_id;
float s_salary;

//constructor
Software_Engg(int id,String name, float salary,int aadhar_no, Address addr)
{
super(name,addr,aadhar_no);
this.s_id=id;
this.s_salary=salary;
}

//get and set s_id
public int gets_id()
{
return s_id;
}
public void sets_id(int n)
{
this.s_id=n;
}

//get and set s_salary
public float gets_sal()
{
return s_salary;
}
public void sets_sal(float n)
{
this.s_salary=n;
}
}


